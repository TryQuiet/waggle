  "scripts": {
    "build": "tsc",
    "version": "git add -A src",
    "postversion": "git push && git push --tags",
    "lint": "eslint ./src/**/*.{ts,tsx,js,jsx}",
    "test": "DEBUG=ipfs:*,waggle:* jest ./src/**/* --runInBand",
